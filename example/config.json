{
    "copy": {
        "demo1": {
            "from": {
                "prefix": "example/copy/source",
                "items": [
                    "**",
                    "*.*"
                ]
            },
            "to": {
                "prefix": "example/copy/target",
                "items": [
                    "demo1"
                ]
            }
        },
        "demo2": {
            "from": {
                "prefix": "example/copy/source",
                "items": [
                    "1.*",
                    "*.pdf",
                    "test*",
                    "dir*",
                    "*test",
                    "r[d.dll$]"
                ]
            },
            "to": {
                "prefix": "example/copy/target",
                "items": [
                    "demo2_1",
                    "demo2_2"
                ]
            }
        },
        "demo3": {
            "from": {
                "prefix": "example/copy/source",
                "items": [
                    "{{VIRABLE}}.*"
                ]
            },
            "to": {
                "prefix": "example/copy/target",
                "items": [
                    "demo3"
                ]
            }
        }
    },
    "buildStyle": {
        "demo1": {
            "from": {
                "prefix": "example/buildstyle/source",
                "item": "a.less"
            },
            "to": {
                "prefix": "example/buildstyle/target",
                "item": "a.qss"
            }
        },
        "demo2": {
            "from": {
                "prefix": "example/buildstyle/source",
                "item": "{{VIRABLE}}.less"
            },
            "to": {
                "prefix": "example/buildstyle/target",
                "item": "{{VIRABLE}}.qss"
            }
        }
    }
}